{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "pack-product-catalog-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/packProductCatalog/PackProductCatalogWebPart.js",
          "manifest": "./src/webparts/packProductCatalog/PackProductCatalogWebPart.manifest.json"
        }
      ]
    },
    "packt-product-search-box-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/packtProductSearchBox/PacktProductSearchBoxWebPart.js",
          "manifest": "./src/webparts/packtProductSearchBox/PacktProductSearchBoxWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "PackProductCatalogWebPartStrings": "lib/webparts/packProductCatalog/loc/{locale}.js",
    "PacktProductSearchBoxWebPartStrings": "lib/webparts/packtProductSearchBox/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
