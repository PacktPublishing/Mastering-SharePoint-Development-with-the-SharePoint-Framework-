{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "pack-product-catalog-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/packProductCatalog/PackProductCatalogWebPart.js",
          "manifest": "./src/webparts/packProductCatalog/PackProductCatalogWebPart.manifest.json"
        }
      ]
    },
    "packt-product-search-box-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/packtProductSearchBox/PacktProductSearchBoxWebPart.js",
          "manifest": "./src/webparts/packtProductSearchBox/PacktProductSearchBoxWebPart.manifest.json"
        }
      ]
    },
    "packt-product-form-customizer-form-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/packtProductFormCustomizer/PacktProductFormCustomizerFormCustomizer.js",
          "manifest": "./src/extensions/packtProductFormCustomizer/PacktProductFormCustomizerFormCustomizer.manifest.json"
        }
      ]
    },
    "packt-product-application-customizer-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/packtProductApplicationCustomizer/PacktProductApplicationCustomizerApplicationCustomizer.js",
          "manifest": "./src/extensions/packtProductApplicationCustomizer/PacktProductApplicationCustomizerApplicationCustomizer.manifest.json"
        }
      ]
    },
    "packt-product-field-customizer-field-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/packtProductFieldCustomizer/PacktProductFieldCustomizerFieldCustomizer.js",
          "manifest": "./src/extensions/packtProductFieldCustomizer/PacktProductFieldCustomizerFieldCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "PackProductCatalogWebPartStrings": "lib/webparts/packProductCatalog/loc/{locale}.js",
    "PacktProductSearchBoxWebPartStrings": "lib/webparts/packtProductSearchBox/loc/{locale}.js",
    "PacktProductFormCustomizerFormCustomizerStrings": "lib/extensions/packtProductFormCustomizer/loc/{locale}.js",
    "PacktProductApplicationCustomizerApplicationCustomizerStrings": "lib/extensions/packtProductApplicationCustomizer/loc/{locale}.js",
    "PacktProductFieldCustomizerFieldCustomizerStrings": "lib/extensions/packtProductFieldCustomizer/loc/{locale}.js"
  }
}
